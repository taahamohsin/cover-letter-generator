{
    "buildCommand": "cd frontend && yarn build",
    "outputDirectory": "frontend/dist",
    "devCommand": null,
    "installCommand": "cd frontend && yarn install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}